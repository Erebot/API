{
    "name": "erebot/erebot-api",
    "description": "API for the Erebot project",
    "keywords": ["IRC", "bot"],
    "homepage": "https://github.com/Erebot/Erebot_API",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "FranÃ§ois Poirotte",
            "email": "clicky@erebot.net",
            "role": "lead"
        }
    ],
    "support": {
        "email": "erebot@erebot.net",
        "issues": "https://github.com/Erebot/Erebot_API/issues",
        "wiki": "http://erebot.github.com/Erebot/",
        "irc": "irc://irc.iiens.net/Erebot",
        "source": "https://github.com/Erebot/Erebot_API"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "require": {
        "php": ">=5.2.2",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-libxml": "*",
        "ext-pcre": "*",
        "ext-Reflection": "*",
        "ext-SimpleXML": "*",
        "ext-sockets": "*",
        "ext-SPL": "*",
        "ext-XML": "*",
        "erebot/erebot-api": "*"
    },
    "suggest": {
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-posix": "*"
    },
    "autoload": {
        "psr-0": {
            "Erebot": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
